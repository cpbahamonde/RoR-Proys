<h1>Apartments</h1>
<table>
    <thead>
        <tr>
            <th>Number</th>
            <th>Building</th>
            <th colspan="3"></th>
        </tr>
    </thead>
    <tbody>
    <% @apartments.each do |apartment| %>
        <tr>
            <td><%= apartment.number %></td>
            <td><%= apartment.building %></td>
            
        </tr>
    <% end %>
    </tbody>
</table>

